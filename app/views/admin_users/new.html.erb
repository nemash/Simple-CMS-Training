<% @page_title = "Create Admin User" %>

<%= link_to("<< Back to list", {:action => 'index'}, :class => 'back-link') %>

<div class="user new">
    <h2>Create User</h2>
    
    <%= form_for(:admin_user, :url => {:action => 'create'}) do |f| %>
    
        <%= render(:partial => "form", :locals => {:f => f}) %>

        <div class="form-buttons">
            <%= submit_tag("Create Editors") %>
        </div>
    <% end %>
</div>
